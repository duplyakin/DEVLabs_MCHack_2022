{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "async-aws/ses": "^1.4",
        "beberlei/doctrineextensions": "^1.2",
        "boshurik/telegram-bot-bundle": "^4.1",
        "composer/package-versions-deprecated": "^1.11",
        "danog/madelineproto": "^5.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^3",
        "eluceo/ical": "^0.16.1",
        "google/apiclient": "^2.0",
        "knplabs/knp-paginator-bundle": "^5.3",
        "knpuniversity/oauth2-client-bundle": "^2.3",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^3.0",
        "league/oauth2-linkedin": "^5.1",
        "oro/doctrine-extensions": "^2.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "predis/predis": "^1.1",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/amazon-mailer": "5.1.*",
        "symfony/amqp-messenger": "5.1.*",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/google-mailer": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/intl": "5.1.*",
        "symfony/mailer": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/mime": "5.1.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/notifier": "5.1.*",
        "symfony/polyfill-intl-messageformatter": "^1.18",
        "symfony/process": "5.1.*",
        "symfony/property-access": "5.1.*",
        "symfony/property-info": "5.1.*",
        "symfony/proxy-manager-bridge": "5.1.*",
        "symfony/redis-messenger": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/string": "5.1.*",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/translation": "5.1.*",
        "symfony/twig-bundle": "^5.1",
        "symfony/ux-chartjs": "^1.3",
        "symfony/validator": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/yaml": "5.1.*",
        "symfonycasts/reset-password-bundle": "^1.1",
        "symfonycasts/verify-email-bundle": "^1.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.1",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "symfony/browser-kit": "^5.1",
        "symfony/css-selector": "^5.1",
        "symfony/debug-bundle": "^5.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/stopwatch": "^5.1",
        "symfony/var-dumper": "^5.1",
        "symfony/web-profiler-bundle": "^5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
