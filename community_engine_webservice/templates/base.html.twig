{% block body %}{% endblock %}
<!-- ======= Footer ======= -->
<footer id="footer" class="section-bg">
    <div class="footer-top">
        <div class="container">

            <div class="row">

                <div class="col-lg-6">

                    <div class="row">

                        <div class="col-sm-6">

                            <div class="footer-info">
                                <h3>Meetsup</h3>
                                <p>{{ 'footer.text'|trans|raw }}</p>
                            </div>

                            {#<div class="footer-newsletter">#}
                            {#<h4>Our Newsletter</h4>#}
                            {#<p>Tamen quem nulla quae legam multos aute sint culpa legam noster magna veniam enim#}
                            {#veniam illum dolore legam minim quorum culpa amet magna export quem.</p>#}
                            {#<form action="" method="post">#}
                            {#<input type="email" name="email"><input type="submit" value="Subscribe">#}
                            {#</form>#}
                            {#</div>#}

                        </div>

                        <div class="col-sm-6">
                            <div class="footer-links">
                                <h4>{{ 'Links'|trans }}</h4>
                                <ul>
                                    <li><a href="/#">{{ 'Home'|trans }}</a></li>
                                    {#<li><a href="/#about">О нас</a></li>#}
                                </ul>
                            </div>

                            <div class="footer-links">
                                <h4>{{ 'Contacts'|trans }}</h4>
                                <p>
                                    <strong>Email:</strong>
                                    <a href="mailto:{{ supportEmail }}">{{ supportEmail }}</a><br />
                                </p>
                            </div>

                            {#<div class="social-links">#}
                            {#<a href="#" class="twitter"><i class="fa fa-twitter"></i></a>#}
                            {#<a href="#" class="facebook"><i class="fa fa-facebook"></i></a>#}
                            {#<a href="#" class="instagram"><i class="fa fa-instagram"></i></a>#}
                            {#<a href="#" class="linkedin"><i class="fa fa-linkedin"></i></a>#}
                            {#</div>#}

                        </div>

                    </div>

                </div>

                <div class="col-lg-6">

                    <div class="form">

                        <h4>{{ 'Feedback'|trans }}</h4>

                        <form action="/contact" method="post" role="form" class="php-email-form">
                            <input type="hidden" name="contact_form[_token]"
                                   value="{{ csrf_token('contact_form') }}"
                            >
                            {% if app.user %}
                                <input type="hidden" name="contact_form[name]" value="{{ app.user.getFullName()|e }}">
                                <input type="hidden" name="contact_form[email]" value="{{ app.user.getActualEmail() }}">
                            {% else %}
                                <div class="form-group">
                                    <input type="text" name="contact_form[name]" class="form-control" id="name"
                                           placeholder="{{ 'Name'|trans }}"
                                           data-rule="minlen:4" data-msg="{{ 'At least 4 characters'|trans }}"/>
                                    <div class="validate"></div>
                                </div>
                                <div class="form-group">
                                    <input type="email" class="form-control" name="contact_form[email]" id="email"
                                           placeholder="Email" data-rule="email"
                                           data-msg="{{ 'Invalid email format'|trans }}"/>
                                    <div class="validate"></div>
                                </div>
                            {% endif %}
                            <div class="form-group">
                                <input type="text" class="form-control" name="contact_form[subject]" id="subject"
                                       placeholder="{{ 'Subject'|trans }}" data-rule="minlen:4"
                                       data-msg="{{ 'At least 4 characters'|trans }}"/>
                                <div class="validate"></div>
                            </div>
                            <div class="form-group">
                                <textarea class="form-control" name="contact_form[message]" rows="5"
                                          data-rule="required"
                                          data-msg="{{ 'Required field'|trans }}"
                                          placeholder="{{ 'Message'|trans }}"></textarea>
                                <div class="validate"></div>
                            </div>

                            <div class="mb-3">
                                <div class="loading">Загрузка</div>
                                <div class="error-message"></div>
                                <div class="sent-message">{{ 'Your message has been sent. Thank!'|trans }}</div>
                            </div>

                            <div class="text-center">
                                <button type="submit" title="Send Message">{{ 'Send'|trans }}</button>
                            </div>
                        </form>

                    </div>

                </div>

            </div>

        </div>
    </div>

    <div class="container">
        <div class="copyright">
            &copy; Copyright <strong>Meetsup</strong>. All Rights Reserved
        </div>
        <div class="credits"></div>
    </div>
</footer><!-- End  Footer -->

