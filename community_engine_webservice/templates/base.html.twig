                {#<a href="{{ path('user_communities') }}">Подтверждение встреч и оценки</a>#}
                {#</li>#}
                {#<li class="{{ app.request.get('_route') == 'call_list' ? 'active' }}">#}
                {#<a href="{{ path('call_list') }}">Мой нетворкинг</a>#}
                {#</li>#}
                {#{% endif %}#}
                {% if app.user %}
                    <li class="drop-down d-none d-lg-block pl-5">
                        <div class="image-cropper header-avatar align-middle"
                             style="
