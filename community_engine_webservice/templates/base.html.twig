            <a href="/">
                <img src="{{ asset('assets/img/logo.svg') }}">MEET<span style="color: #1bb1dc">S</span>UP
            </a>
        </h1>
        <nav class="main-nav d-none d-lg-block">
            <ul>
                {#{% if app.request.get('_route') == 'index' %}#}
                {#<li class="active"><a href="/">{% trans %}Index{% endtrans %}</a></li>#}
